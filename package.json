{
  "name": "groundhogg-js",
  "version": "1.0.1",
  "description": "TypeScript client for the Groundhogg CRM API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "groundhogg",
    "wordpress",
    "crm",
    "typescript",
    "sdk",
    "email",
    "contacts"
  ],
  "author": "Parth Koshti",
  "license": "MIT",
  "dependencies": {
    "ky": "^1.8.1"
  },
  "devDependencies": {
    "@vitest/ui": "^3.1.2",
    "dotenv": "^16.5.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.2"
  }
}
