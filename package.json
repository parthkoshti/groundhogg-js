{
  "name": "groundhogg-js",
  "version": "1.0.6",
  "description": "TypeScript client for the Groundhogg CRM API",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "groundhogg",
    "wordpress",
    "crm",
    "typescript",
    "sdk",
    "email",
    "contacts"
  ],
  "author": "Parth Koshti",
  "license": "MIT",
  "dependencies": {
    "ky": "^1.8.1"
  },
  "devDependencies": {
    "@vitest/ui": "^3.1.2",
    "dotenv": "^16.5.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.2"
  }
}
